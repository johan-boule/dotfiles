alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias ln='ln -i'

alias clearback="echo -ne '\ec\e[3J'" # clear screen and scrollback

alias treea='command tree -AF' # -I *~
alias treefia='treea -fi'
alias tree='treea -a'
alias treefi='treefia -a'
